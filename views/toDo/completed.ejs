<div class="row">
    <div class="col-md-8 offset-md-2">
        <table class="table table-hover table-striped table-bordered">
            <tr>
                <th>Title</th>
                <th>Category Name</th>
                <th>Completed Date</th>
            </tr>
                <% for(let i=0; i < completedToDos.length; i++) { %>
                    <tr                    >
                        <td><%= completedToDos[i].Title %></td>
                        <td><%= completedToDos[i].Name %></td>
                        <td><%= completedToDos[i].CompletedDate.toDateString() %></td>
                    </tr>
                <%}%>
        </table>
    </div>
</div>