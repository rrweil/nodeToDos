<div class="row">
    <div class="col-md-8 offset-md-2">
        <table class="table table-hover table-striped table-bordered">
            <tr>
                <th>Title</th>
                <th>Category Name</th>
                <th>Completed Date</th>
            </tr>
                <% completedToDos.forEach(function(completedToDo) { %>
                    <tr                    >
                        <td><%= completedToDo.Title %></td>
                        <td><%= completedToDo.Name %></td>
                        <td><%= completedToDo.CompletedDate.toDateString() %></td>
                    </tr>
                  <% }); %>
        </table>
    </div>
</div>